{
	"schema_version": 1,
	"quilt_loader": {
		"group": "io.github.sleepy_evelyn",
		"id": "more_mod_tabs",
		"version": "${version}",
		"metadata": {
			"name": "More Mod Tabs",
			"description": "Allows players and modpack makers to automatically add new custom tabs for mods that don't include their own.",
			"contributors": {
				"sleepy-evelyn": "Sleepy Evelyn"
			},
			"contact": {
				"homepage": "https://modrinth.com/mod/more-mod-tabs",
				"issues": "https://github.com/sleepy-evelyn/More-Mod-Tabs/issues",
				"sources": "https://github.com/sleepy-evelyn/More-Mod-Tabs"
			},
			"icon": "assets/example_mod/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"client_init": "io.github.sleepy_evelyn.more_mod_tabs.MMT"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.19.1"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=7.0.2"
			},
			{
				"id": "minecraft",
				"versions": ">=1.20"
			}
		]
	},
	"mixin": "more_mod_tabs.mixins.json",
	"minecraft": {
		"environment": "client"
	}
}
